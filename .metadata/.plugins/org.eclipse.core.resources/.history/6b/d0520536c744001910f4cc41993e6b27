package lab02;
/** A class that represents a path via pursuit curves. */
public class Path {

    // TODO
	public Point curr =new Point(0,0);
	public Point next;
	
	public Path (double  x , double y) {
		next.x =  x;
		next.y =  y;
 	}
	public void iterate (double dx ,double dy) {
		curr = next ;
		next.x = next.x+dx;
		next.y = next.y+dy;
		
	}

}
